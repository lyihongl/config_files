execute pathogen#infect()
syntax on
filetype plugin indent on
set number
hi MatchParen cterm=none ctermbg=111 ctermfg=125
set background=dark
hi String ctermfg=141
hi Cursor ctermbg=red ctermfg=white
set guicursor=i:ver100-iCursor
let &t_SI = "\<Esc>[6 q"
let &t_SR = "\<Esc>[4 q"
let &t_EI = "\<Esc>[2 q""]"
set wrap
set linebreak
set nolist " list disables linebreak
set textwidth=0
set wrapmargin=0
